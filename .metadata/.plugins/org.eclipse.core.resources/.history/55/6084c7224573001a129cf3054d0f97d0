package com.OopsConcept;

//Example 2: Overloading - Different Number of parameters in argument list 
public class SampleTest1 {
	
	//Main method to test Method Overloading
	 public static void main(String args[])
	   {
		 //Creating the instance of the class DisplayOverloading1
	       DisplayOverloading1 obj = new DisplayOverloading1();
	       
	       // Calling the Overloaded Methods.
	       obj.disp('a');
	       obj.disp('a',10);
	   }

}
