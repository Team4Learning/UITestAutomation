package com.OopsConcept;

public class DisplayOverloading1 {
	
	//method with  1 parameter of type char
	 public void disp(char c)
	    {
	         System.out.println(c); // prints the character
	    }
	 // Method with 2 arguments of type char and int 
	    public void disp(char c, int num)  
	    {
	         System.out.println(c + " "+num); // prints both character and integer
	    }

}
