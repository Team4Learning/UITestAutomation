package com.OopsConcept;

public class Main {
	
	
	public static void main(String[] args) {
		
		// Creating object Of Class Company
		Company c = new Company("Kaaru");
		//Reference c1 pointing to the same object c
		Company c1 = c;
		
		System.out.println(c.getEname()); // Prints the name : Vija
		System.out.println(c1.setEname("Nishu"));
		
		
		
	}

}
